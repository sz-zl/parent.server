<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sz.yk.dao.CityMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sz.p2p.entity.City">
        <id column="CITY_NO" property="cityNo" />
        <result column="CITY_ID" property="cityId" />
        <result column="CITY_NAME" property="cityName" />
        <result column="CTIY_GRADE" property="ctiyGrade" />
        <result column="PARENT_CITY_ID" property="parentCityId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        CITY_NO, CITY_ID, CITY_NAME, CTIY_GRADE, PARENT_CITY_ID
    </sql>

</mapper>
