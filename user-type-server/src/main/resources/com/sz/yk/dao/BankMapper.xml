<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sz.yk.dao.BankMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sz.p2p.entity.Bank">
        <id column="BANK_NO" property="bankNo" />
        <result column="BANK_ID" property="bankId" />
        <result column="BANK_NAME" property="bankName" />
        <result column="DISABLE_BANK" property="disableBank" />
        <result column="CITY_ID" property="cityId" />
        <result column="REMARK" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        BANK_NO, BANK_ID, BANK_NAME, DISABLE_BANK, CITY_ID, REMARK
    </sql>

</mapper>
